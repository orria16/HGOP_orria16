{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prefer-const": 0,
        "indent": [2, "tab"],
        "no-tabs": 0,
        "quotes": 0,
        "no-var": 0,
        "max-len": 0,
        "space-before-function-paren": 0,
        "arrow-parens": 0,
        "eol-last": 0,
        "comma-dangle": 0,
        "require-jsdoc": 0,
        "object-curly-spacing": 0,
        "valid-jsdoc": 0,
        "no-unused-vars": 0,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"]
    }
}