{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len":[1,187,2],
        "require-jsdoc" : 0,
        "no-unused-vars": 0,
        "no-tabs": 0,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"]
    }
}